{
  "name": "budget-app",
  "version": "1.0.0",
  "description": "Aplicativo de gerenciamento de orÃ§amento pessoal",
  "scripts": {
    "install:all": "cd client && npm install && cd ../server && npm install",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "build:production": "npm run build:server && npm run build:client",
    "start:client": "cd client && npm run preview",
    "start:server": "cd server && npm start",
    "start:production": "cd server && npm start",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:production": "docker build -f Dockerfile.production -t budget-app:latest .",
    "prisma:generate": "cd server && npx prisma generate",
    "prisma:migrate": "cd server && npx prisma migrate dev",
    "prisma:migrate:deploy": "cd server && npx prisma migrate deploy",
    "prisma:studio": "cd server && npx prisma studio",
    "deploy:railway": "railway deploy"
  },
  "keywords": [
    "budget",
    "finance",
    "react",
    "nodejs",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
