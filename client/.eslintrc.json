{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "ignorePatterns": [
        "dist",
        "node_modules"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "no-console": "warn"
    }
}